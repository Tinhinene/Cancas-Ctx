@model IEnumerable<Inspinia_MVC5.ViewModels.EcritFactViewModel>
@{
    ViewBag.Title = "DataTables";

}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>DataTables</h2>
        <ol class="breadcrumb">
            <li class="active">
                <strong>Ecrit_Fact</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of Ecrit_Fact</h5>

                </div>
                <div class="ibox-content">
                    <table class="table table-striped">
                        <tr>

                            <th>
                                Num Cotisant
                            </th>

                            <th>
                                Solde
                            </th>

                            <th></th>
                        </tr>
                        @foreach (var item in Model)
                        {
                            <tr>


                                <td>
                                    @Html.DisplayFor(modelItem => item.N_Cotisant)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.solde)
                                </td>

                            </tr>
                        }
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
